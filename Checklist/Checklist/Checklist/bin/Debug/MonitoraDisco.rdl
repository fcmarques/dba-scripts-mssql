  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>RSQLADM</DataSourceName>
        <CommandText>select SERVIDOR, DISCO, CAPACIDADE,FREESPACE,CONVERT(date,DATA) AS DATA
from discmon where data &gt; convert(date,getdate())
and disco not in ('c:\', 'z:\')
and capacidade &gt;10
and freespace &gt;10
and freespace &lt;= capacidade*0.15
order by servidor, disco</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="servidor">
          <DataField>SERVIDOR</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="disco">
          <DataField>DISCO</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="capacidade">
          <DataField>CAPACIDADE</DataField>
          <rd:TypeName>System.Int16</rd:TypeName>
        </Field>
        <Field Name="freespace">
          <DataField>FREESPACE</DataField>
          <rd:TypeName>System.Int16</rd:TypeName>
        </Field>
        <Field Name="data">
          <DataField>DATA</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
