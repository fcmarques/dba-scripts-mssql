    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>RSQLADM</DataSourceName>
        <CommandText>select SERVIDOR, DISCO, CAPACIDADE,FREESPACE,CONVERT(date,DATA) AS DATA
from discmon where data &gt; convert(date,getdate())
and disco not in ('c:\', 'z:\')
and capacidade &gt;10
and freespace &gt;10
and freespace &lt;= capacidade*0.15
order by servidor, disco</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
