  <DataSets>
    <DataSet Name="DBOwner">
      <Query>
        <DataSourceName>CentralDB</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Environment">
            <Value>=Parameters!Environment.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT z.InstanceName, z.DBName,  z.DBSizeInMB, z.DBOwner, v.environment
FROM (select y.* from    (SELECT InstanceName, MAX(DateAdded) AS Rundate
                  FROM      DB.DatabaseInfo
                  GROUP BY InstanceName) AS x INNER JOIN
                  [CentralDB].DB.DatabaseInfo AS y ON x.Rundate = y.DateAdded AND x.InstanceName = y.InstanceName)z
inner join(
select Distinct ServerName, InstanceName, Environment, Description, BusinessOwner from  [CentralDB].[Svr].[ServerList]) v ON Z.InstanceName = V.InstanceName AND z.DBOwner &lt;&gt; 'sa'
Where v.Environment IN (@Environment)
Order by Z.InstanceName
</CommandText>
      </Query>
      <Fields>
        <Field Name="InstanceName">
          <DataField>InstanceName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DBName">
          <DataField>DBName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DBSizeInMB">
          <DataField>DBSizeInMB</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="DBOwner">
          <DataField>DBOwner</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="environment">
          <DataField>environment</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Environ">
      <Query>
        <DataSourceName>CentralDB</DataSourceName>
        <CommandText>SELECT Distinct  [Environment]
  FROM [CentralDB].[Svr].[ServerList]
  Group By Environment
  Order By Environment</CommandText>
      </Query>
      <Fields>
        <Field Name="Environment">
